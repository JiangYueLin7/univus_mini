      {
        "pagePath": "pages/Inbox/index",
        "text": "Inbox",
        "iconPath": "images/inbox.png",
        "selectedIconPath": "images/inbox-active.png"
      },
      {
        "pagePath": "pages/Calendar/index",
        "text": "Calendar",
        "iconPath": "images/calendar.png",
        "selectedIconPath": "images/calendar-active.png"
      }
